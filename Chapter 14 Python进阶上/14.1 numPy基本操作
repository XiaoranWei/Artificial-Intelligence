#数据类型必须统一

import numpy as np

a_list = list(range(10))
b = np.array(a_list)
type(b)
//numpy.ndarray

a = np.zeros(10)
print(type(a))
a.dtype
//dtype('float64')

a = np.zeros(10,dtype=int)
print(type(a))
a.dtype
//dtype('int64')

np.full()

np.zeros_like(a)
np.ones_like(a)
np.full_like(a,4.12,dtype=float)
